{
  "name": "gatsby-source-personio-xml",
  "version": "1.0.0",
  "description": "Gatsby source plugin to use a Personio XML feed as a data source",
  "main": "lib/gatsby-node.js",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib && copyfiles package.json README.md lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kolossal-io/gatsby-source-personio-xml"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "personio",
    "xml"
  ],
  "author": "Marijan Barkic <marijan@kolossal.io>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.22.0",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
